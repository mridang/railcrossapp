{
  "name": "railcross",
  "version": "0.0.1",
  "scripts": {
    "build": "sls package",
    "deploy": "sls deploy --aws-profile=mridang",
    "start": "npx nest start",
    "test": "jest --verbose --config=jest.config.js --runInBand",
    "test:watch": "npm run test -- --watch",
    "test:debug": "jest --verbose --config=jest.config.js --runInBand --detectOpenHandles",
    "format": "prettier --write \"**/*.{js,ts,jsx,tsx,json,css,md,html}\"",
    "format:check": "prettier --check \"**/*.{js,ts,jsx,tsx,json,css,md,html}\"",
    "lint": "npx eslint --ext .jsx,.js,.tsx,.ts .",
    "lint:fix": "npx eslint --ext .jsx,.js,.tsx,.ts . --fix",
    "serverless": "serverless"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^1.18.1",
    "@aws-sdk/client-scheduler": "^3.511.0",
    "@aws-sdk/client-secrets-manager": "^3.511.0",
    "@codegenie/serverless-express": "^4.3.0",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/cqrs": "^10.2.6",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/terminus": "^10.2.3",
    "@octokit/core": "^5.1.0",
    "@octokit/plugin-rest-endpoint-methods": "^10.4.0",
    "@octokit/rest": "^20.0.2",
    "@slack/bolt": "^3.16.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "express-handlebars": "^7.1.2",
    "hbs": "^4.2.0",
    "probot": "^12.3.3",
    "reflect-metadata": "^0.2.1"
  },
  "engines": {
    "node": "20.x"
  },
  "repository": {
    "url": "https://github.com/mridang/railcrossapp"
  },
  "overrides": {
    "eslint-config-standard-with-typescript": {
      "@typescript-eslint/eslint-plugin": "^7.0.0"
